﻿{
	"$ref": "#/definitions/TrackableEntity",
    "$schema": "http://json-schema.org/draft-07/schema#",
    "definitions": {
        "Address": {
            "description": "Physical address of the entity",
            "properties": {
                "city": {
                    "description": "City name",
                    "type": "string"
                },
                "country": {
                    "$ref": "#/definitions/Code",
                    "description": "Country name"
                },
                "postalCode": {
                    "description": "Postal code",
                    "type": "string"
                },
                "province": {
                    "description": "Province name",
                    "type": "string"
                },
                "street": {
                    "description": "Street name",
                    "type": "string"
                }
            },
            "type": "object"
        },
        "AssetStatus": {
            "description": "Enumerables for asset status",
            "enum": [
                "accepted",
                "cancelled",
                "executing",
                "finished",
                "planned",
                "rejected",
                "requested"
            ],
            "type": "string"
        },
        "Code": {
            "description": "Class that stores a code and its name",
            "properties": {
                "assetType": {
                    "$ref": "#/definitions/EntityType",
                    "description": "Asset type"
                },
                "code": {
                    "description": "Item Code",
                    "type": "string"
                },
                "delete": {
                    "description": "Deleted or not",
                    "type": "boolean"
                },
                "history": {
                    "description": "Object change history",
                    "items": {},
                    "type": "array"
                },
                "inBC": {
                    "description": "Whether or not it is stored on the Blockchain",
                    "type": "boolean"
                },
                "messageID": {
                    "description": "Message identifier",
                    "type": "string"
                },
                "name": {
                    "description": "Item Name",
                    "type": "string"
                },
                "reportedByCode": {
                    "description": "Code of the identity that has reported it",
                    "type": "string"
                },
                "reportedByID": {
                    "description": "Identifier of the entity who reported it",
                    "type": "string"
                },
                "reportedByRole": {
                    "$ref": "#/definitions/OrganizationType",
                    "description": "Role of the identity that has reported it"
                },
                "userId": {
                    "description": "User identifier",
                    "type": "string"
                },
                "version": {
                    "description": "Object version",
                    "type": "string"
                }
            },
            "type": "object"
        },
        "EntityType": {
            "description": "Enumerables for entity types",
            "enum": [
                "ack",
                "charges",
                "customsProcedure",
                "document",
                "locationFacility",
                "masterData",
                "message",
                "office",
                "operation",
                "service",
                "shipment",
                "storageFacility",
                "storedItem",
                "trackableEvent",
                "transport",
                "transportUnit",
                "user",
                "vehicle"
            ],
            "type": "string"
        },
        "EventInfo": {
            "description": "Information of a logistics event",
            "properties": {
                "ID": {
                    "description": "Event id",
                    "type": "string"
                },
                "delete": {
                    "description": "Deleted or not",
                    "type": "boolean"
                },
                "eventType": {
                    "$ref": "#/definitions/EventType",
                    "description": "Event type"
                },
                "location": {
                    "$ref": "#/definitions/Location",
                    "description": "Location data"
                },
                "locationID": {
                    "description": "Location id",
                    "type": "string"
                },
                "name": {
                    "$ref": "#/definitions/ReferenceType",
                    "description": "Event name"
                },
                "performedTime": {
                    "description": "Performed time",
                    "format": "date-time",
                    "type": "string"
                }
            },
            "type": "object"
        },
        "EventType": {
            "description": "Enumerables for event status",
            "enum": [
                "accepted",
                "arrived",
                "cancelled",
                "changed",
                "declared",
                "delayed",
                "delivered",
                "departed",
                "detained",
                "discharged",
                "dispatched",
                "doorClosed",
                "doorOpened",
                "finished",
                "gateIn",
                "gateOut",
                "inspected",
                "loaded",
                "ordered",
                "pickedUp",
                "planned",
                "position",
                "received",
                "registered",
                "rejected",
                "released",
                "requested",
                "sensor",
                "shunting",
                "started",
                "stopped"
            ],
            "type": "string"
        },
        "GeoJSON": {
            "description": "Geographic coordinates of an element",
            "properties": {
                "coordinates": {
                    "description": "Array formed by each of the coordinate numbers",
                    "items": {
                        "type": "number"
                    },
                    "type": "array"
                },
                "type": {
                    "description": "Coordinate Type",
                    "type": "string"
                }
            },
            "type": "object"
        },
        "Location": {
            "description": "Physical and temporary location of a logistics element",
            "properties": {
                "address": {
                    "$ref": "#/definitions/Address",
                    "description": "Physical address"
                },
                "assetType": {
                    "$ref": "#/definitions/EntityType",
                    "description": "Asset type"
                },
                "ata": {
                    "format": "date-time",
                    "type": "string"
                },
                "atd": {
                    "format": "date-time",
                    "type": "string"
                },
                "eta": {
                    "format": "date-time",
                    "type": "string"
                },
                "etd": {
                    "format": "date-time",
                    "type": "string"
                },
                "history": {
                    "description": "Object change history",
                    "items": {},
                    "type": "array"
                },
                "inBC": {
                    "description": "Whether or not it is stored on the Blockchain",
                    "type": "boolean"
                },
                "location": {
                    "type": "string"
                },
                "locationName": {
                    "type": "string"
                },
                "messageID": {
                    "description": "Message identifier",
                    "type": "string"
                },
                "position": {
                    "$ref": "#/definitions/GeoJSON",
                    "description": "Geographical position"
                },
                "positionTime": {
                    "description": "Time instant in which the sample was captured",
                    "format": "date-time",
                    "type": "string"
                },
                "reportedByCode": {
                    "description": "Code of the identity that has reported it",
                    "type": "string"
                },
                "reportedByID": {
                    "description": "Identifier of the entity who reported it",
                    "type": "string"
                },
                "reportedByRole": {
                    "$ref": "#/definitions/OrganizationType",
                    "description": "Role of the identity that has reported it"
                },
                "rta": {
                    "description": "Requested (rta), estimated (eta), actual (ata) time of arrival",
                    "format": "date-time",
                    "type": "string"
                },
                "rtd": {
                    "description": "Requested (rtd), estimated (etd), actual (atd) time of departure",
                    "format": "date-time",
                    "type": "string"
                },
                "sequence": {
                    "description": "Sequence number",
                    "type": "number"
                },
                "type": {
                    "$ref": "#/definitions/LocationType",
                    "description": "Physical location type"
                },
                "userId": {
                    "description": "User identifier",
                    "type": "string"
                },
                "version": {
                    "description": "Object version",
                    "type": "string"
                }
            },
            "type": "object"
        },
        "LocationType": {
            "description": "Enumerables for location types",
            "enum": [
                "arrival",
