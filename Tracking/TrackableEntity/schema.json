                "discharge",
                "dispatch",
                "document",
                "freightCharges",
                "gateIn",
                "gateOut",
                "licensePlate",
                "load",
                "pickUp",
                "pin",
                "portCall",
                "procedure",
                "reception",
                "serviceCharges",
                "shipment",
                "storageCharges",
                "storedItem",
                "stowage",
                "system",
                "trailerPlate",
